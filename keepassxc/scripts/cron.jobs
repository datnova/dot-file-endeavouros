# auto sync databases
@reboot ~/.config/keepassxc/scripts/sync_cloud.sh >> ~/.config/keepassxc/logs/sync_cloud.log 2>&1
*/10 * * * * ~/.config/keepassxc/scripts/sync_cloud.sh >> ~/.config/keepassxc/logs/sync_cloud.log 2>&1

# auto sync backups
@reboot ~/.config/keepassxc/scripts/backups_cloud.sh >> ~/.config/keepassxc/logs/backups_cloud.log 2>&1
@hourly ~/.config/keepassxc/scripts/backups_cloud.sh >> ~/.config/Keepassxc/logs/backups_cloud.log 2>&1
